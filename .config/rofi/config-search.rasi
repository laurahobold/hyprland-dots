/* Rofi Config for Google Search) */

@import "~/.config/rofi/master-config.rasi"  

/* ---- Window ---- */
window {
	width: 40%;
    y-offset: 6px;
    location: north;
}

/* ---- Inputbar ---- */
inputbar {
    enabled: true;
}

/* ---- Mainbox ---- */
mainbox {
    children: [ "entry"];
}

/* ---- Entry ---- */ 
entry {
	placeholder: "Google Search";
}

